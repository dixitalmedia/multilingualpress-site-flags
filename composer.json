{
    "name": "inpsyde/multilingualpress-site-flags",
    "description": "An external module to add additional functionality to MultilingualPress. The module will enable displaying flags in the language switcher, it can be activated from MultilingualPress settings",
    "type": "mlp-mod",
    "license": "MIT",
    "authors": [
        {
            "name": "Inpsyde GmbH",
            "email": "hello@inpsyde.com",
            "homepage": "https://inpsyde.com",
            "role": "Company"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "brain/monkey": "~2.3",
        "inpsyde/php-coding-standards": "^1.0.0-beta1",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "^8.5",
        "wp-cli/i18n-command": "dev-master",
        "squizlabs/php_codesniffer": "^3.2",
        "consolidation/robo": "^3.0.4",
        "scssphp/scssphp": "~1.3.0"
    },
    "autoload": {
        "psr-4": {
            "Inpsyde\\MultilingualPress\\Flags\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inpsyde\\MultilingualPress\\Flags\\Tests\\Unit\\": "tests/php/unit"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
